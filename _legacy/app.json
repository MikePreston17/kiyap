{
  "addons": [
    "jawsdb",
    "mongolab"
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    }
  ],
  "description": "master branch",
  "env": {
    "JAWSDB_URL": {
      "required": true
    },
    "MONGODB_URI": {
      "required": true
    },
    "REACT_APP_API_KEY": {
      "required": true
    },
    "REACT_APP_AUTH_DOMAIN": {
      "required": true
    },
    "REACT_APP_DATABASE_URL": {
      "required": true
    },
    "REACT_APP_GOOGLE_MAPS_API_KEY": {
      "required": true
    },
    "REACT_APP_MESSAGING_SENDER_ID": {
      "required": true
    },
    "REACT_APP_PROJECT_ID": {
      "required": true
    },
    "REACT_APP_STORAGE_BUCKET": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "name": "kiyap",
  "scripts": {
  },
  "stack": "heroku-18"
}
